package data;

import org.newdawn.slick.opengl.Texture;
import static helpers.Artist.*;

public enum TowerType {

	CannonRed(QuickLoad("cannonBase"), QuickLoad("cannonGun"), 10);
	
	
	Texture cannonTexture, baseTexture;
	int damage;
	
	TowerType(Texture cannonTexture, Texture baseTexture, int damage) {
		this.cannonTexture = cannonTexture;
		this.baseTexture = baseTexture;
		this.damage = damage;
	}
}
