package data;

import static helpers.Artist.BeginSession;

import org.lwjgl.opengl.Display;


public class Boot {

	public Boot() {
		
		BeginSession();
		
		while(!Display.isCloseRequested()) {
			
			
			Display.update();
			Display.sync(60);
			
		}
		
		Display.destroy();
	}
	
	
	public static void main(String[] args) {
		new Boot();
	}
}
